program Michal_Vojtech_Pascal1a;

var
a,b,c, nej: integer;
begin
   nej:= 0;
    writeln('Napis 3 libovolna cisla:');
    readln(a);
    readln(b);
    readln(c);

    if a>nej then nej:= a;

    if b>nej then nej:= b;

    if c>nej then nej:= c;

    writeln('nejvetsi cislo je: ',nej);
    readln();
end.


          
